- query =
    UPDATE(
      LINK(transport_useful_demand_cars, transport_useful_demand_passenger_kms),
      share,
      DIVIDE(USER_INPUT(), 100)
    )
- share_group = passenger_transport
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value = present:V(transport_useful_demand_cars,share_of_transport_useful_demand_passenger_kms) * 100
- step_value = 0.1
- unit = %
- update_period = future
