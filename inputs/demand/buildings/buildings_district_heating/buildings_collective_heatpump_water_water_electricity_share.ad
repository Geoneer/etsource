      V(OUTPUT_SLOTS(LOOKUP(buildings_collective_heatpump_water_water_electricity),steam_hot_water), "links.detect{|l| !l.flexible? }"),
      share, USER_INPUT() / 100.0
    )
- share_group = district_heating_buildings
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:V(buildings_collective_heatpump_water_water_electricity,share_of_buildings_heat_network_steam_hot_water) * 100
- step_value = 0.1
- unit = %
- update_period = future
