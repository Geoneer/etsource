# unit = euro/MWh

- query =
    present_value = present:DIVIDE(V(energy_transport_hydrogen_pipelines, fixed_costs_per(:plant)),V(energy_transport_hydrogen_pipelines, "typical_input_capacity * full_load_hours"));
    relative_value = USER_INPUT() / present_value;
      EACH(
      UPDATE_WITH_FACTOR(V(energy_transport_hydrogen_pipelines), initial_investment, relative_value),
      UPDATE_WITH_FACTOR(V(energy_transport_hydrogen_pipelines), fixed_operation_and_maintenance_costs_per_year, relative_value)
      )
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:DIVIDE(V(energy_transport_hydrogen_pipelines, fixed_costs_per(:plant)),V(energy_transport_hydrogen_pipelines, "typical_input_capacity * full_load_hours"))
- step_value = 0.01
- unit = euro
- update_period = future
