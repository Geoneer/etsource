# add electricity production from CHPs to final demand electricity
# add electricity use of geothermal heater to final demand electricity

- query =
      DATASET_INPUT(input_agriculture_electricity_demand) +
      (DATASET_INPUT(agriculture_chp_engine_network_gas_demand) * NODE_EFFICIENCY(agriculture_chp_engine_network_gas, output, electricity)) +
      (DATASET_INPUT(agriculture_chp_engine_biogas_demand) * NODE_EFFICIENCY(agriculture_chp_engine_biogas, output, electricity)) +
      (DATASET_INPUT(agriculture_chp_supercritical_wood_pellets_demand) * NODE_EFFICIENCY(agriculture_chp_supercritical_wood_pellets, output, electricity)) +
      (DATASET_INPUT(input_agriculture_geothermal_demand) * NODE_EFFICIENCY(agriculture_geothermal, input, electricity)) +
      (DATASET_INPUT(input_agriculture_heatpump_demand) * NODE_EFFICIENCY(agriculture_heatpump_water_water_ts_electricity, input, electricity))
