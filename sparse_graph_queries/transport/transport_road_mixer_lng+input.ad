
- query =
    total_road_mixer_lng_demand = USER_INPUT(transport_final_demand_for_road_lng_demand) + USER_INPUT(transport_final_demand_for_road_bio_lng_demand)
      { lng: USER_INPUT(transport_final_demand_for_road_bio_lng_demand) / total_road_mixer_lng_demand,
        bio_lng: USER_INPUT(transport_final_demand_for_road_bio_lng_demand) / total_road_mixer_lng_demand }
