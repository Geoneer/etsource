
- query =
    total_shipping_mixer_lng_demand = USER_INPUT(transport_final_demand_for_shipping_lng_demand) + USER_INPUT(transport_final_demand_for_shipping_bio_lng_demand)
      { lng: USER_INPUT(transport_final_demand_for_shipping_bio_lng_demand) / total_shipping_mixer_lng_demand,
        bio_lng: USER_INPUT(transport_final_demand_for_shipping_bio_lng_demand) / total_shipping_mixer_lng_demand }
