
- query =
    total_shipping_mixer_diesel_demand = USER_INPUT(input_transport_ship_diesel_demand) + USER_INPUT(input_transport_ship_biodiesel_demand) + USER_INPUT(transport_final_demand_heavy_fuel_oil_demand)
      { lng: 0.0,
        bio_lng: 0.0,
        diesel: USER_INPUT(input_transport_ship_diesel_demand) / total_shipping_mixer_diesel_demand,
        biodiesel: USER_INPUT(input_transport_ship_diesel_demand) / total_shipping_mixer_biodiesel_demand,
        heavy_fuel_oil: USER_INPUT(transport_final_demand_heavy_fuel_oil_demand) / total_shipping_mixer_diesel_demand }
