
- query =
    chemical_fertilizers_electricity = DATASET_INPUT(input_industry_chemical_fertilizers_scaling_factor) * PARENT_VALUE(industry_final_demand_for_chemical_fertilizers_electricity, input_of_electricity) / 1_000_000;
    chemical_refineries_electricity =  DATASET_INPUT(input_industry_chemical_refineries_scaling_factor) * PARENT_VALUE(industry_final_demand_for_chemical_refineries_electricity, input_of_electricity) / 1_000_000;
    chemical_electricity_demand = chemical_fertilizers_electricity + chemical_refineries_electricity + DATASET_INPUT(input_industry_chemical_other_electricity_demand);
    chemical_electricity_demand = IF(chemical_electricity_demand > 0.0, chemical_electricity_demand, 1.0);

    chemical_fertilizers_electricity_demand / chemical_electricity_demand
    