
- query =
    chemical_fertilizers_crude_oil = DATASET_INPUT(input_industry_chemical_fertilizers_scaling_factor) * PARENT_VALUE(industry_final_demand_for_chemical_fertilizers_crude_oil, input_of_crude_oil) / 1_000_000;
    chemical_refineries_crude_oil =  DATASET_INPUT(input_industry_chemical_refineries_scaling_factor) * PARENT_VALUE(industry_final_demand_for_chemical_refineries_crude_oil, input_of_crude_oil) / 1_000_000;
    chemical_crude_oil_demand = chemical_fertilizers_crude_oil + chemical_refineries_crude_oil + DATASET_INPUT(input_industry_chemical_other_crude_oil_demand);
    chemical_crude_oil_demand = IF(chemical_crude_oil_demand > 0.0, chemical_crude_oil_demand, 1.0);

    chemical_fertilizers_crude_oil_demand / chemical_crude_oil_demand
    